{
  "expo": {
    "name": "zellix",
    "slug": "zellix",
    "version": "1.3.7",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.skg.zellix",
      "bundleNumber": "1.0.0",
      "supportsTablet": true,
      "buildNumber": "58",
      "permissions": [
        "READ_CALL_LOG"
      ],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Zellix to access your location for scheduling and attending meetings efficiently.",
        "NSLocationAlwaysUsageDescription": "Allow Zellix to access your location in the background to support location-based meeting features."
      }
    },
    "android": {
      "package": "com.skg.zellix",
      "versionCode": 53,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.READ_CALL_LOG",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon1.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload the documents",
          "cameraPermission": "$(PRODUCT_NAME) requires access to the Camera allows you to take photo to upload the documents"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for conduct meeting."
        }
      ]
    ],
    "owner": "swavish",
    "runtimeVersion": "1.3.6",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "url": "https://u.expo.dev/9b8838cd-39d5-4148-a221-a81701015be2"
    },
    "extra": {
      "eas": {
        "projectId": "e2a7dab4-4f52-4b2f-92b8-5d31227185a3"
      }
    }
  }
}
